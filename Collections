#List
names = ['name1', 'name2']
print(len(names)) #Of items
names.insert(0,'name0') #push the names to the right and put 'name0' into index[0]
names.sort()   #change the list
names[1:3] #Range
names[ :3]
names[1: ]

#Array


#Dictionaries - are like Caplets / Synonym
person1 = {'first': 'name1'}
person1['last'] = 'lastname1'

#Adding Dictionaries to a List
people = []
people.append(person1)
people.append( { 'first: 'name2', 'last': 'lastname2' } )
